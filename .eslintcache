) {\n      const newBasket = basket.filter((i) => i.id !== id);\n      updateBasket([...newBasket]);\n    } else {\n      const newBasket = basket;\n      newBasket[index].quantity = newBasket[index].quantity + num;\n      updateBasket([...newBasket]);\n    }\n  };\n\n  ////REMOVE ALL OF ITEM\n  const removeAllOfItem = (id) => {\n    if (\n      window.confirm(\n        \"Are you sure you want to remove all of this item from your basket?\"\n      )\n    ) {\n      const newBasket = basket.filter((i) => i.id !== id);\n      updateBasket([...newBasket]);\n    }\n  };\n\n  //////////MAIN OUTPUT /////////////\n\n  return (\n    <div className=\"App\">\n      <BrowserRouter>\n        <Basket\n          basket={basket}\n          adjustQuantity={(id, num) => adjustQuantity(id, num)}\n          removeAllOfItem={(id) => removeAllOfItem(id)}\n        />\n        <Search setFilter={(value) => setFilter(value)} />\n        <Fragment>\n          <Switch>\n            <Route\n              exact\n              path=\"/\"\n              render={(props) => (\n                <ShopList\n                  {...props}\n                  addToBasket={(item) => addToBasket(item)}\n                />\n              )}\n            />\n            <Route\n              exact\n              path=\"/search/:filter\"\n              render={(props) => (\n                <ShopList\n                  {...props}\n                  searchFilter={filter}\n                  addToBasket={(item) => addToBasket(item)}\n                />\n              )}\n            />\n            <Route\n              exact\n              path=\"/viewproduct/:id\"\n              render={(props) => (\n                <ProductPage\n                  {...props}\n                  addToBasket={(item) => addToBasket(item)}\n                />\n              )}\n            />\n            <Route\n              exact\n              path=\"/checkout\"\n              render={(props) => <Checkout {...props} basket={basket} />}\n            />\n          </Switch>\n        </Fragment>\n      </BrowserRouter>\n    </div>\n  );\n}\n\nexport default App;\n","/Users/davehillman/Documents/shop/src/Basket.js",["53"],"import React, { useState } from \"react\";\nimport { withRouter, Link } from \"react-router-dom\";\nimport { makeStyles } from \"@material-ui/core/styles\";\nimport Button from \"@material-ui/core/Button\";\n\nimport \"./styles/css/basket.css\";\n\nconst useStyles = makeStyles({\n  root: {\n    background: \"linear-gradient(45deg, #FE6B8B 30%, #FF8E53 90%)\",\n    borderRadius: 3,\n    border: 0,\n    color: \"white\",\n    height: 48,\n    padding: \"0 30px\",\n    boxShadow: \"0 3px 5px 2px rgba(255, 105, 135, .3)\",\n  },\n  payNowButton: {\n    padding: \"3rem\",\n    backgroundColor: \"rgba(144,200,200)\",\n    \"&:hover\": {\n      backgroundColor: \"rgba(144,200,220)\",\n    },\n  },\n});\n\nfunction Basket({ basket, adjustQuantity, removeAllOfItem }) {\n  const [showBasket, setShowBasket] = useState(false);\n  const classes = useStyles();\n  return (\n    <div className={`basket ${showBasket && \"visible\"}`}>\n      <div className=\"basket__main\">\n        <h2>Basket</h2>\n        <ul className=\"basket__list\">\n          {basket && basket.length > 0 ? (\n            basket.map((item) => {\n              return (\n                <li className=\"basket__item\">\n                  <span className=\"qty\">{item.quantity}</span>\n                  <span className=\"item-name\">{item.details.name}</span>\n                  <span className=\"price\">\n                    {(item.details.price * item.quantity).toFixed(2)}\n                  </span>\n                  <div className=\"basket__controls\">\n                    <button\n                      className=\"basket__btn\"\n                      onClick={(id, num) => adjustQuantity(item.id, -1)}\n                    >\n                      -\n                    </button>\n                    <button\n                      className=\"basket__btn\"\n                      onClick={(id, num) => adjustQuantity(item.id, +1)}\n                    >\n                      +\n                    </button>\n\n                    <button\n                      className=\"basket__btn\"\n                      onClick={(id) => removeAllOfItem(item.id)}\n                    >\n                      <i className=\"fas fa-trash basket__btn\"></i>\n                    </button>\n                  </div>\n                </li>\n              );\n            })\n          ) : (\n            <p>Empty! Click + on an item to add to basket</p>\n          )}\n        </ul>\n        {!basket.length <= 0 && (\n          <Link\n            to=\"/checkout\"\n            variant=\"contained\"\n            color=\"primary\"\n            className={classes.payNowButton}\n            onClick={() => setShowBasket(false)}\n          >\n            Pay Now\n          </Link>\n        )}\n      </div>\n      <div\n        className=\"basket__tab\"\n        id=\"basketTab\"\n        onClick={() => {\n          setShowBasket(!showBasket);\n        }}\n      >\n        <div className=\"basket__tab-icon\">\n          <i className=\"fas fa-shopping-basket basket-icon\"></i>\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default withRouter(Basket);\n","/Users/davehillman/Documents/shop/src/Search.js",[],"/Users/davehillman/Documents/shop/src/Checkout.js",["54","55"],"import React, { Fragment } from \"react\";\nimport { withRouter } from \"react-router-dom\";\n\nimport \"./styles/css/checkout.css\";\nimport { makeStyles } from \"@material-ui/core/styles\";\nimport Button from \"@material-ui/core/Button\";\n\nimport { stock } from \"./stock\";\n\nconst useStyles = makeStyles({\n  pay: {\n    padding: \"1.5rem 2rem\",\n    fontSize: \"1rem\",\n  },\n});\n\nfunction Checkout({ match, basket }) {\n  const classes = useStyles();\n\n  return (\n    <div className=\"checkout\">\n      <h1>CHECKOUT</h1>\n      